{
  "@context": {
    "@vocab": "https://core.kg.ebrains.eu/vocab/query/",
    "query": "https://schema.hbp.eu/myQuery/",
    "propertyName": {
      "@id": "propertyName",
      "@type": "@id"
    },
    "merge": {
      "@type": "@id",
      "@id": "merge"
    },
    "path": {
      "@id": "path",
      "@type": "@id"
    }
  },
  "meta": {
    "type": "https://openminds.ebrains.eu/core/ContentTypePattern",
    "description": "Automatically generated by fairgraph"
  },
  "structure": [
    {
      "path": "@id",
      "filter": {
        "op": "EQUALS",
        "parameter": "id"
      }
    },
    {
      "path": "https://core.kg.ebrains.eu/vocab/meta/space",
      "propertyName": "query:space",
      "filter": {
        "op": "EQUALS",
        "value": "collab-foobar"
      }
    },
    {
      "path": "@type"
    },
    {
      "path": "https://openminds.ebrains.eu/vocab/lookupLabel",
      "propertyName": "vocab:lookupLabel",
      "sort": true
    },
    {
      "path": "https://openminds.ebrains.eu/vocab/contentType",
      "propertyName": "vocab:contentType",
      "structure": [
        {
          "path": "@id"
        },
        {
          "path": "@type"
        }
      ]
    },
    {
      "path": "https://openminds.ebrains.eu/vocab/regex",
      "propertyName": "vocab:regex"
    },
    {
      "path": {
        "@id": "https://openminds.ebrains.eu/vocab/contentTypePattern",
        "reverse": true
      },
      "propertyName": "vocab:contentTypePattern",
      "ensureOrder": true,
      "structure": [
        {
          "path": "@id"
        },
        {
          "path": "@type"
        }
      ]
    }
  ]
}